@model IEnumerable<Proyecto_PrograAvanzada.Models.Asignacion>

<h1>Asignaciones</h1>

<p>
    <a asp-action="Create">Agregar Nueva</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FechaAsignacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaInicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaCierre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdSolicitudNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdTecnicoNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FechaAsignacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaInicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaCierre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdSolicitudNavigation.IdSolicitud)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdTecnicoNavigation.IdTecnico)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdAsignacion">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.IdAsignacion">Detalles</a> |
                <a asp-action="Delete" asp-route-id="@item.IdAsignacion">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
